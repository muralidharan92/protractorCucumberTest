    "test": "protractor outputjs/config/conf.js",
    "posttest": "node outputjs/config/reportConfig.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/jasmine": "^3.6.1",
    "chromedriver": "86.0.4240.22",
    "log4js": "^6.3.0",
    "webdriver-manager": "^12.1.7"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.0.0-rc.0",
    "@types/chai": "^4.2.14",
    "@types/cucumber": "^6.0.1",
    "@types/node": "~12.19.0",
    "chai": "^4.2.0",
    "cucumber": "^6.0.5",
    "cucumber-html-reporter": "^5.2.0",
    "jasmine": "3.6.3",
    "jest": "^26.6.3",
    "protractor": "7.0.0",
    "protractor-cucumber-framework": "^6.2.1",
    "typescript": "3.6.3"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/unit/**/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageDirectory": "./dist/reports/coverage",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverageFrom": [
      "**/src/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 90,
        "statements": 90
      }
    }
  }
